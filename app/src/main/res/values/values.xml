<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Android Account Authenticator Config -->
    <string name="android_auth_account_type">com.alfresco.content.app</string>
    <string name="android_auth_label">Alfresco Content Services</string>

    <!-- Settings Config -->
    <string name="settings_account_key">account</string>
    <string name="settings_theme_key">theme</string>
    <string name="settings_version_key">version</string>

    <string name="settings_theme_light">light</string>
    <string name="settings_theme_dark">dark</string>
    <string name="settings_theme_system">system</string>

    <string-array name="settings_theme_values">
        <item>@string/settings_theme_light</item>
        <item>@string/settings_theme_dark</item>
        <item>@string/settings_theme_system</item>
    </string-array>
</resources>
